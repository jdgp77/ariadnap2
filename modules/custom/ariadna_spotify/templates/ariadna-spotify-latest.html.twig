<!-- Add you custom twig html here -->
<h2>Nuevos Lanzamientos</h2>
<div class="grid">
  <div class="row">
    {% for item in items %}
      <div class="col-12 col-sm-6 col-md-3">
        <img src="{{ item.images[0].url }}" />
        <h3>{{ item.name }}</h3>
        <a href="/artista/{{ item.artists[0].id }}">{{ item.artists[0].name }}</a>
      </div>
    {% endfor %}
  </div>
</div>